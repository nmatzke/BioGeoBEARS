			} else {#
			force_sparse = FALSE#
			}#
		} else {#
		force_sparse = BioGeoBEARS_run_object$force_sparse#
		}#
#
	if (force_sparse == TRUE)#
		{#
		cat("\nNote: force_sparse is set to TRUE; length(states_list)=", length(states_list), "\n", sep="")#
		}#
